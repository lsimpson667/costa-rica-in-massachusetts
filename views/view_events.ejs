<%- include('partials/head'); -%>
    
        <button><a class="return-to-admin" href="/admin" >Return to Admin Options</a></button>
        <br><br>
        <div class="admin_page_container">
            <h1>Event List</h1>


            <p>Click on an event to see it's detiails.</p>

            <% all_events.forEach( (single_event) => { %>

            <h4 class="admin-links"> 
                <a href="/admin/event_details/<%= single_event._id%>"><%= single_event.event_name %></a>                        
            </h4>
            <% }) %>
        </div>
    </body>
</html>
   

